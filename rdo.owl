    <!-- http://webprotege.stanford.edu/RB3bxfNs0KGdXacZxMLhjzb -->


    <owl:DatatypeProperty rdf:about="http://webprotege.stanford.edu/RB3bxfNs0KGdXacZxMLhjzb">
